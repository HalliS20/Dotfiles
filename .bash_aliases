#!/bin/zsh

alias n='nvim'
alias rst='exec zsh'
alias ls='lsd --group-dirs first --color=auto'
alias l='lsd --group-dirs first --color=auto -1'
alias ll='lsd -l --group-dirs first --color=auto'
alias la='lsd -la --group-dirs first --color=auto'

alias pip='pip3'
alias python='python3'

source ~/.files/.cars
source ~/.files/.ezgit
